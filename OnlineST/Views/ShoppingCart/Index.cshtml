
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model OnlineST.Models.Pagination.PaginatedCollection<CartProduct>

<div>
    @if (Model.Collection is not null)
    {
        @foreach (CartProduct cartProduct in Model.Collection)
        {
            <div class="card">
                <div class="card-body">@cartProduct.Product?.Name</div>
                <div class="card-footer"></div>
            </div>
        }
    }

</div>


